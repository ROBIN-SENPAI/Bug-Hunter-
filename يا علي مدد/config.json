{
  "project": {
    "name": "AlBaTTaR BUGS",
    "version": "1.0.0",
    "author": "ROBIN (@ll bUg)",
    "description": "AI-Powered Bug Bounty Hunting Tool"
  },
  
  "scan_settings": {
    "default_scan_type": "normal",
    "max_threads": 100,
    "default_threads": 10,
    "timeout": 30,
    "max_retries": 3,
    "delay_between_requests": 0.5,
    "max_requests_per_second": 10,
    "follow_redirects": true,
    "max_redirects": 5,
    "verify_ssl": false
  },
  
  "scan_profiles": {
    "quick": {
      "description": "Fast scan with basic checks",
      "threads": 20,
      "timeout": 15,
      "scanners": ["sqli", "xss", "lfi"],
      "depth": 1
    },
    "normal": {
      "description": "Balanced scan with common vulnerabilities",
      "threads": 10,
      "timeout": 30,
      "scanners": ["sqli", "xss", "lfi", "rfi", "xxe", "ssrf", "idor"],
      "depth": 2
    },
    "full": {
      "description": "Comprehensive scan with all scanners",
      "threads": 15,
      "timeout": 60,
      "scanners": "all",
      "depth": 3
    },
    "deep": {
      "description": "Thorough deep scan with fuzzing",
      "threads": 5,
      "timeout": 120,
      "scanners": "all",
      "depth": 5,
      "enable_fuzzing": true
    }
  },
  
  "ai_settings": {
    "enabled": false,
    "default_model": "auto",
    "models": {
      "gpt-4": {
        "provider": "openai",
        "model_name": "gpt-4-turbo-preview",
        "max_tokens": 4096,
        "temperature": 0.3,
        "api_key_env": "OPENAI_API_KEY"
      },
      "claude": {
        "provider": "anthropic",
        "model_name": "claude-3-sonnet-20240229",
        "max_tokens": 4096,
        "temperature": 0.3,
        "api_key_env": "ANTHROPIC_API_KEY"
      },
      "local": {
        "provider": "local",
        "model_name": "llama3-8b",
        "model_path": "models/llama3-8b.gguf"
      }
    },
    "features": {
      "false_positive_filtering": true,
      "context_analysis": true,
      "payload_generation": true,
      "vulnerability_classification": true,
      "exploit_suggestion": true,
      "learning_enabled": true
    },
    "cache_enabled": true,
    "cache_ttl": 86400
  },
  
  "vulnerability_settings": {
    "severity_thresholds": {
      "critical": 9.0,
      "high": 7.0,
      "medium": 4.0,
      "low": 0.1
    },
    "confidence_threshold": 70,
    "enable_cvss_scoring": true,
    "report_info_findings": true
  },
  
  "scanners": {
    "injection": {
      "sqli": {
        "enabled": true,
        "techniques": ["union", "boolean", "time", "error", "stacked"],
        "payloads_file": "payloads/sqli/",
        "timeout": 30,
        "time_delay": 5
      },
      "nosql": {
        "enabled": true,
        "databases": ["mongodb", "couchdb", "redis"],
        "payloads_file": "payloads/nosql/"
      },
      "command_injection": {
        "enabled": true,
        "os_types": ["linux", "windows"],
        "payloads_file": "payloads/rce/",
        "time_delay": 5
      },
      "code_injection": {
        "enabled": true,
        "languages": ["php", "python", "ruby", "nodejs"],
        "payloads_file": "payloads/code_injection/"
      },
      "template_injection": {
        "enabled": true,
        "engines": ["jinja2", "twig", "smarty", "freemarker"],
        "payloads_file": "payloads/template_injection/"
      }
    },
    
    "xss": {
      "reflected": {
        "enabled": true,
        "payloads_file": "payloads/xss/reflected.txt"
      },
      "stored": {
        "enabled": true,
        "payloads_file": "payloads/xss/stored.txt",
        "recheck_delay": 5
      },
      "dom": {
        "enabled": true,
        "use_selenium": true,
        "payloads_file": "payloads/xss/dom_based.txt"
      },
      "blind": {
        "enabled": true,
        "callback_server": "https://xss.example.com"
      }
    },
    
    "file_inclusion": {
      "lfi": {
        "enabled": true,
        "payloads_file": "payloads/lfi/linux_lfi.txt",
        "wrappers": true,
        "log_poisoning": true
      },
      "rfi": {
        "enabled": true,
        "payloads_file": "payloads/lfi/rfi.txt",
        "remote_server": "http://attacker.com"
      },
      "path_traversal": {
        "enabled": true,
        "depth": 10,
        "payloads_file": "payloads/lfi/traversal.txt"
      }
    },
    
    "xxe": {
      "enabled": true,
      "techniques": ["classic", "blind", "oob"],
      "payloads_file": "payloads/xxe/",
      "oob_server": "http://xxe.example.com"
    },
    
    "ssrf": {
      "enabled": true,
      "techniques": ["basic", "blind", "cloud_metadata"],
      "payloads_file": "payloads/ssrf/",
      "internal_ips": ["127.0.0.1", "localhost", "169.254.169.254"]
    },
    
    "authentication": {
      "auth_bypass": {
        "enabled": true,
        "techniques": ["sql", "nosql", "logic"]
      },
      "weak_credentials": {
        "enabled": true,
        "username_list": "wordlists/common_usernames.txt",
        "password_list": "wordlists/common_passwords.txt",
        "max_attempts": 50
      },
      "jwt": {
        "enabled": true,
        "attacks": ["none_algorithm", "key_confusion", "weak_secret"]
      },
      "session": {
        "enabled": true,
        "checks": ["fixation", "hijacking", "timeout"]
      }
    },
    
    "access_control": {
      "idor": {
        "enabled": true,
        "parameter_types": ["numeric", "uuid", "hash"],
        "test_range": 100
      },
      "privilege_escalation": {
        "enabled": true,
        "types": ["horizontal", "vertical"]
      },
      "cors": {
        "enabled": true,
        "test_origins": ["null", "attacker.com"]
      }
    },
    
    "business_logic": {
      "race_condition": {
        "enabled": true,
        "concurrent_requests": 50
      },
      "mass_assignment": {
        "enabled": true,
        "test_parameters": ["role", "admin", "is_admin", "privileges"]
      },
      "payment_logic": {
        "enabled": true,
        "test_scenarios": ["price_manipulation", "discount_abuse"]
      }
    },
    
    "api": {
      "abuse": {
        "enabled": true,
        "rate_limit_test": true
      },
      "graphql": {
        "enabled": true,
        "introspection": true,
        "depth_limit_test": true
      },
      "rest": {
        "enabled": true,
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
      }
    }
  },
  
  "waf_bypass": {
    "enabled": true,
    "detect_waf": true,
    "techniques": {
      "encoding": ["url", "double_url", "unicode", "hex", "base64"],
      "obfuscation": ["case_variation", "comment_injection", "whitespace"],
      "timing": ["random_delay", "slow_requests"],
      "ip_rotation": true,
      "user_agent_rotation": true
    },
    "known_wafs": [
      "cloudflare",
      "modsecurity",
      "imperva",
      "akamai",
      "aws_waf",
      "barracuda"
    ]
  },
  
  "reconnaissance": {
    "subdomain_enumeration": {
      "enabled": true,
      "sources": ["crt.sh", "virustotal", "securitytrails", "dns_bruteforce"],
      "wordlist": "wordlists/subdomains.txt"
    },
    "port_scanning": {
      "enabled": false,
      "ports": "1-1000",
      "top_ports": 100
    },
    "directory_bruteforce": {
      "enabled": true,
      "wordlist": "wordlists/common_directories.txt",
      "extensions": [".php", ".asp", ".aspx", ".jsp", ".html", ".js"],
      "recursive": true,
      "max_depth": 3
    },
    "technology_detection": {
      "enabled": true,
      "detect_server": true,
      "detect_framework": true,
      "detect_cms": true,
      "detect_waf": true
    }
  },
  
  "fuzzing": {
    "enabled": false,
    "types": ["mutation", "generation", "feedback"],
    "max_iterations": 1000,
    "interesting_responses": [500, 501, 502, 503]
  },
  
  "reporting": {
    "formats": ["json", "html"],
    "output_directory": "reports/",
    "include_poc": true,
    "include_remediation": true,
    "include_references": true,
    "executive_summary": true,
    "technical_details": true,
    "screenshots": true
  },
  
  "integrations": {
    "slack": {
      "enabled": false,
      "webhook_url_env": "SLACK_WEBHOOK_URL",
      "notify_on": ["critical", "high"]
    },
    "discord": {
      "enabled": false,
      "webhook_url_env": "DISCORD_WEBHOOK_URL",
      "notify_on": ["critical", "high"]
    },
    "telegram": {
      "enabled": false,
      "bot_token_env": "TELEGRAM_BOT_TOKEN",
      "chat_id_env": "TELEGRAM_CHAT_ID",
      "notify_on": ["critical", "high"]
    },
    "email": {
      "enabled": false,
      "smtp_server": "smtp.gmail.com",
      "smtp_port": 587,
      "from_email_env": "EMAIL_FROM",
      "to_email_env": "EMAIL_TO",
      "password_env": "EMAIL_PASSWORD"
    },
    "bug_bounty_platforms": {
      "hackerone": {
        "enabled": false,
        "api_key_env": "HACKERONE_API_KEY"
      },
      "bugcrowd": {
        "enabled": false,
        "api_key_env": "BUGCROWD_API_KEY"
      }
    }
  },
  
  "database": {
    "type": "sqlite",
    "path": "data/scans.db",
    "save_results": true,
    "save_history": true,
    "retention_days": 365
  },
  
  "proxy": {
    "enabled": false,
    "url": null,
    "rotation": false,
    "proxy_file": null,
    "proxy_type": "http"
  },
  
  "headers": {
    "user_agent": "AlBaTTaR-BUGS/1.0 (Security Scanner)",
    "accept": "*/*",
    "accept_language": "en-US,en;q=0.9",
    "accept_encoding": "gzip, deflate",
    "connection": "close",
    "custom_headers": {}
  },
  
  "behavioral": {
    "stealth_mode": false,
    "aggressive_mode": false,
    "ethical_mode": true,
    "respect_robots_txt": true,
    "max_scan_duration": 3600,
    "auto_stop_on_ban": true
  },
  
  "logging": {
    "level": "INFO",
    "log_file": "logs/albattar_bugs.log",
    "log_requests": false,
    "log_responses": false,
    "max_log_size": "10MB",
    "backup_count": 5
  },
  
  "performance": {
    "cache_enabled": true,
    "cache_size": "100MB",
    "memory_limit": "2GB",
    "cpu_limit": 80
  },
  
  "scope": {
    "scope_file": "scope.txt",
    "strict_scope": true,
    "allow_subdomains": true,
    "blocked_domains": []
  },
  
  "payloads": {
    "custom_payloads_dir": "payloads/custom/",
    "encoding": ["raw", "url", "double_url", "base64"],
    "max_payload_length": 10000
  },
  
  "wordlists": {
    "directory": "wordlists/",
    "custom_wordlists": {}
  }
}